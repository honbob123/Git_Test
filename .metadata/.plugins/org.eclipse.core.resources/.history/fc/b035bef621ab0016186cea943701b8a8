package com.silentpeople.ex1;

import java.util.Scanner;

public class testclass {
	String name;
	int age;
	
	public testclass(){
		name = "babo";
		age = 20;
	}
	
	public void printAge() {
		System.out.println(age);
	}
	
	public void scanprintname() {
		Scanner sc = new Scanner(System.in);
		name = sc.nextLine();
		System.out.println(name);
	}
}
